    "@glazed/did-datastore": "^0.3.0-alpha.0",
    "@glazed/tile-loader": "^0.2.0-alpha.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "@mui/styles": "^5.4.1",
    "@stablelib/random": "^1.0.0",
    "dids": "^3.0.0-alpha.0",
    "key-did-provider-ed25519": "^2.0.0-alpha.0",
    "key-did-resolver": "^2.0.0-alpha.0",
