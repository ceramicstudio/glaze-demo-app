    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
