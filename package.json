{
  "name": "glaze-demo-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ceramicstudio.github.io/glaze-demo-app",
  "scripts": {
    "ceramic": "ceramic daemon",
    "create-model": "node --experimental-json-modules ./scripts/create-model.mjs",
